{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "db:migrate:deploy": {
      "outputs": []
    },
    "db:push": {
      "outputs": []
    },
    "db:seed": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "generate": {
      "dependsOn": ["^generate"]
    },
    "lint": {
      "outputs": []
    }
  },
  "globalDependencies": [
    "$DOMAIN",
    "$FLY_REGION",
    "$NODE_ENV",
    "$PORT",
    "$PRIMARY_REGION",
    "$SESSION_SECRET",
    "$STEAM_API_KEY"
  ]
}
