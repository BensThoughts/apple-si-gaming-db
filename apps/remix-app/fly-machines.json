{
  "config": {
    "app_name": "steamed-apples-remix-app-machines",
    "org_slug": "steamed-apples",
    "image": "registry.fly.io/steamed-apples-remix-app-machines:deployment-01GVEMXVBEQJJYB9SE4K296STT",
    "env": {
      "NODE_ENV": "production",
      "PORT": "8080",
      "PRIMARY_REGION": "lax",
      "LOG_LEVEL": "debug",
      "LOGGER_SERVICE_LABEL": "apps-remix-app"
    },
    "services": [
      {
        "ports": [
          {
            "port": 443,
            "handlers": [
              "tls",
              "http"
            ]
          },
          {
            "port": 80,
            "handlers": [
              "http"
            ]
          }
        ],
        "protocol": "tcp",
        "internal_port": 8080
      }
    ],
    "checks": {
        "httpget": {
            "type": "http",
            "port": 8080,
            "method": "GET",
            "path": "/healthcheck",
            "interval": "15s",
            "timeout": "10s"
        }
    }
  }
}